---
import './header.css';
const { origin } = Astro.props;
import Logo from '../Logo.astro';


interface MenuItem {
  link: URL;
  icon: string;
  content: string;
}


---

<header class="header">
	<div class="header__container">
		<Logo name="Taillard Marie"/>
		<nav class="links"   tabindex="0.1">
			<button class="menu__btn" >Menu</button>
			<ul class="header__list">
				{
					origin.map((data: MenuItem) => (
						<li>
							<a class="menu__link" href={data.link}>
								<span class="icon__container" >
									<iconify-icon
										class="icons"
										icon={data.icon}
										width="20"
										height="20"
									/>
									{data.content}
								</span>
							</a>
						</li>
					))
				}
			</ul>
		</nav>
	</div>
</header>
